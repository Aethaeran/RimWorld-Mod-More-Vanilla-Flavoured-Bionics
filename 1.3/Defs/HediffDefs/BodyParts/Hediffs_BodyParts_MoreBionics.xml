      <ThingDef>BionicLung</ThingDef>
      <HediffDef>BionicLung</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing bionic lung.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>BionicLung</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>BionicLung</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Lung</li>
    </appliedOnFixedBodyParts>
    <addsHediff>BionicLung</addsHediff>
  </RecipeDef>

